<% if user_signed_in? %>
  <div class="search_form">
    <%= form_with url: user_search_path, local: true, method: :get do |f| %>
      <%= f.text_field :word %>
      <%#= f.select :range, options_for_select([['User'], ['Book']]) %>
      <%= f.hidden_field :search, value: "partial_match" %>
      <%= f.submit "検索", class: "btn btn-primary" %>
    <% end %>
  </div>
<% end %>