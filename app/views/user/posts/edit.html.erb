<h1>編集画面</h1>

<%= form_with model: @post, url: user_post_path, method: :patch do |f| %>

  <h4>投稿</h4>
  <%= f.text_area :body %>
  <div class="field">
    <%= f.label"タグ (,で区切ると複数タグ登録できます)" %>
    <%= f.text_field :name,value: @tag_list,class:"form-control"%>
  </div>

  <%= f.submit '保存' %>

<% end %>