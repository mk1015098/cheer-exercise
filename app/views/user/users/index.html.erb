<div class='container px-5 px-md-0'>
  <div class='row'>
    <div class='col-md-12'>
      <h1 style= "font-family: fantasy">Users</h1>
      <table class='table table-inverse'>
        <tbody>
      　　<% @users.each do |user| %>
        　　<% unless user.name == "guestuser" %>
          　　<tr>
                <td><%= image_tag user.get_image, size:"100x100" %></td>
                <td><%= link_to user.name, user_user_path(user.id) %></td>
                <td><%= user.introduction %></td>
              </tr>
        　　<% end %>
          <% end %>
        </tbody>
      </table>
      <%= paginate @users %>
    </div>
  </div>
</div>